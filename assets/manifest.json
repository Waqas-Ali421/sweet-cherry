{
  "manifest_version": 2,
  "name": "Sweet Cherry",
  "version": "0.1.0",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icon/16.png",
      "48": "icon/48.png",
      "128": "icon/128.png"
    }
  },
  "permissions": ["activeTab", "storage"]
}
